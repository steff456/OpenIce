<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="120.0" prefWidth="100.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
  <bottom>
    <Label fx:id="text" alignment="CENTER" contentDisplay="CENTER" text="Label" textAlignment="CENTER" wrapText="true" BorderPane.alignment="CENTER" />
  </bottom>
  <center>
    <ImageView fx:id="icon" fitHeight="0.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true" />
  </center>
</BorderPane>
