        <Button fx:id="close" cancelButton="true" mnemonicParsing="false" text="Close" />
      </left>
      <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" />
      </padding>
      <right>
        <Button fx:id="start" defaultButton="true" mnemonicParsing="false" text="Start" />
