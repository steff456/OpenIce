<BorderPane fx:controller="org.mdpnp.apps.testapp.CreateAdapter" fx:id="main" prefHeight="-1" prefWidth="-1" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" >
  <bottom>
    <BorderPane prefHeight="-1.0" prefWidth="-1.0">
